package com.company;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int n = scanner.nextInt();
        powerOf(a, n);
    }
    public static int powerOf(int a, int n) {
        int res = 1;
        for (int i=1; i<=n; i++) {
            if (n == 0) {
               res = 1;
            } else {
                res = a * res;
            }
        }
        System.out.println(res);
        return res;
    }
}
